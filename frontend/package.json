    "clean": "rimraf dist",
    "dev": "set NODE_ENV=development && webpack-dev-server --devtool eval --progress --colors --hot --config webpack/webpack.dev.config.js --content-base ../app --port 3000",
    "build": "set NODE_ENV=production && webpack -p --progress --colors --config webpack/webpack.prod.config.js && [ -d 'dist_build' ] && rm -rf dist/ && mv dist_build/ dist/",
    "tar:staging": "set NODE_ENV=staging && webpack -p --progress --colors --config webpack/webpack.tar.config.js",
    "tar:prod": "set NODE_ENV=production && webpack -p --progress --colors --config webpack/webpack.tar.config.js",
    "deploy:staging": "./sh/deploy.sh exchangebuddy-staging /home/ubuntu/www staging",
    "deploy:prod": "./sh/deploy.sh exchangebuddy-prod /var/www/ExchangeBuddy production"
  },
